{
  "name": "tibco-gateway-grpc",
  "type": "mashling:trigger",
  "ref": "github.com/TIBCOSoftware/mashling/ext/flogo/trigger/grpc",
  "version": "0.0.1",
  "title": "gRPC Trigger Usage",
  "author": "Naresh Kumar Thota <nthota@tibco.com>",
  "description": "Simple gRPC Trigger for mashling gateway",
  "settings": [
    {
      "name": "port",
      "type": "integer",
      "required": true
    },
    {
      "name": "protoName",
      "type": "string",
      "required": true
    },
    {
      "name": "serviceName",
      "type": "string",
      "required": true
    },
    {
      "name": "enableTLS",
      "type": "boolean"
    },
    {
      "name": "serverCert",
      "type": "string"
    },
    {
      "name": "serverKey",
      "type": "string"
    }
  ],
  "outputs": [
    {
      "name": "params",
      "type": "params"
    },
    {
      "name": "grpcData",
      "type": "object"
    },
    {
      "name": "content",
      "type": "any"
    }
  ],
  "handler": {
    "settings": [
      {
        "name": "autoIdReply",
        "type": "boolean"
      },
      {
        "name": "useReplyHandler",
        "type": "boolean"
      },
      {
        "name": "methodName",
        "type": "string"
      }
    ]
  }
}
